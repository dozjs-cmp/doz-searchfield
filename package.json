{
    "name": "doz-searchfield",
    "version": "0.0.0",
    "description": "A search field for DOZ",
    "main": "dist/bundle.js",
    "scripts": {
        "release:before": "npm run-script test",
        "release:major": "webpack --env.major --mode=production && npm run-script release:publish",
        "release:minor": "webpack --env.minor --mode=production && npm run-script release:publish",
        "release:patch": "webpack --env.patch --mode=production && npm run-script release:publish",
        "release:publish": "echo git commit -a -m BUILD && version-to-tag.sh && npm publish",
        "build:development": "webpack --progress --mode=development",
        "build:production": "webpack --progress --mode=production",
        "watch": "webpack --watch --progress --mode=development",
        "test": "mocha test"
    },
    "keywords": [
        "doz",
        "component",
        "search",
        "input"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dozjs-cmp/doz-searchfield.git"
    },
    "bugs": {
        "url": "https://github.com/dozjs-cmp/doz-searchfield/issues"
    },
    "homepage": "https://github.com/dozjs-cmp/doz-searchfield#readme",
    "author": "Fabio Ricali",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "bejs": "^2.0.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.11",
        "doz": "^1.1.1",
        "ignore-styles": "^5.0.1",
        "jsdoc-to-markdown": "^4.0.1",
        "jsdom": "^11.6.2",
        "jsdom-global": "^3.0.2",
        "mocha": "^5.0.4",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.2.6",
        "unminified-webpack-plugin": "^2.0.0",
        "webpack": "^4.12.0",
        "webpack-auto-inject-version": "^1.1.0",
        "webpack-cli": "^3.0.8"
    },
    "dependencies": {}
}
